					<!--<core:Icon src="sap-icon://menu" text="Boards" size="1em" id="icon0" press="openBoardSearch"/>-->
					<Button xmlns="sap.m" text="Boards" id="button089" icon="sap-icon://menu2" press="openBoardSearch"/>
