<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="scp.com.saparate.controller.jobs">
	<App>
		<pages>
			<Page title="List of Build Pipelines">
				<headerContent>
					<OverflowToolbar id="otb1dd">
						<ToolbarSpacer/>
						<Button icon="sap-icon://create" text="Creat New PipeLine" press="navigatetoCreatePipeline"/>
					</OverflowToolbar>
					<Toolbar width="100%" id="toolbar2">
						<content/>
					</Toolbar>
				</headerContent>
				<content>
					<Table items="{path: 'Jobs>/'}">
						<columns>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Build Pipelines"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Created On"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Modified on"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Created By"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Modified By"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Action"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text=""/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{Jobs>name}"/>
									<Text text="12-Oct-2019 18:20"/>
									<Text text="12-Oct-2019 19:20"/>
									<!--<ObjectStatus class="sapUiSmallMarginBottom" text="Product Damaged" icon="sap-icon://status-negative" state="Error"/>-->
									<Button icon="sap-icon://goalseek" text="Build Results" press="handleshowBuilddetails"/>
									<Button icon="sap-icon://physical-activity" text="Trigger Build" press="initiateTriggerJob"/>
									<ObjectStatus xmlns="sap.m" text="test" state="Information" id="status0" icon="sap-icon://accounting-document-verification" title="jjj"/>
									
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>