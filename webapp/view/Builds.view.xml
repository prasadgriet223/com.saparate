<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="scp.com.saparate.controller.Builds">
	<App>
		<pages>
			<Page id="idPipelineBuilds" title="" showNavButton="false" navButtonPress="navigatetoPrevious">
				<customHeader>
					<Bar id="bar0">
						<contentLeft>
							<Breadcrumbs xmlns="sap.m" currentLocationText="" links="{/}" id="idbreadcrumbs" separatorStyle="GreaterThan">
								<Link emphasized="true" text="{name}" href="{link}" press="onLinkPressNavigate"/>
							</Breadcrumbs>
						</contentLeft>
					<contentMiddle>
					    <Text xmlns="sap.m" text="Text" id="idtxtDashboard"/>
					</contentMiddle>
					</Bar>
				</customHeader>
				<content>
					<Table id="idPipeLineBuildResults" items="{Jobdetails>/}" alternateRowColors="true" itemPress="handleSelectionChange">
						<columns>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Pipeline"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Build"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Estimated Duration"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Duration"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Result"/>
							</Column>
							<Column demandPopin="true" minScreenWidth="Tablet">
								<Text text="Time"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem type="Navigation">
								<cells>
									<ObjectStatus xmlns="sap.m" text="{Jobdetails>name}" state="Information" icon="sap-icon://process"/>
									<ObjectStatus xmlns="sap.m" text="{Jobdetails>number}" state="Information" title="Build #"/>
									<ObjectStatus xmlns="sap.m" text="{ parts : [ 'Jobdetails>estimatedDuration'], formatter: '.formatter.addSec' }" state="Information" icon="sap-icon://lateness"/>
									<ObjectStatus xmlns="sap.m" text="{ parts : [ 'Jobdetails>duartion'], formatter: '.formatter.addSec' }" state="Information" icon="sap-icon://lateness"/>
									<GenericTag text="{Jobdetails>buildResult}" design="StatusIconHidden" status="{ parts : [ 'Jobdetails>buildResult' ], formatter: '.formatter.buildStatus' }" valueState="{ parts : [ 'Jobdetails>buildResult' ], formatter: '.formatter.buildStatus' }"/>
									<ObjectStatus xmlns="sap.m" state="Information" text="{ parts : [ 'Jobdetails>timeStamp'], formatter: '.formatter.formatDate'}" icon="sap-icon://date-time"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>